{
  "TCP": {
    "443": {
      "HTTPS": true
    }
  },
  "Web": {
    "{{ tailscale_serve_quadlet_hostname }}:443": {
      "Handlers": {
        "/": {
          "Proxy": "http://{{ tailscale_serve_quadlet_service_name }}:{{ tailscale_serve_quadlet_service_port }}"
        }
      }
    }
  },
  "AllowFunnel": {
    "{{ tailscale_serve_quadlet_hostname }}:443": false
  }
}
